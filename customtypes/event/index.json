{
	"id": "event",
	"label": "Event",
	"repeatable": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "Slug",
					"placeholder": "eglinton-book-launch"
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title"
				}
			},
			"image": {
				"type": "Image",
				"config": {
					"constraint": {
						"width": 1280,
						"height": 720
					},
					"thumbnails": [],
					"label": "Image"
				}
			},
			"date_and_time": {
				"type": "Timestamp",
				"config": {
					"label": "Date and time"
				}
			},
			"address": {
				"type": "Text",
				"config": {
					"label": "Address",
					"placeholder": "e.g. Neal St Espresso, WC2H 9PS"
				}
			},
			"content": {
				"type": "StructuredText",
				"config": {
					"multi": "paragraph,strong,em,hyperlink",
					"allowTargetBlank": true,
					"label": "Content"
				}
			},
			"eventbrite_url": {
				"type": "Link",
				"config": {
					"label": "Eventbrite URL",
					"select": null
				}
			}
		}
	},
	"status": true,
	"format": "custom"
}
